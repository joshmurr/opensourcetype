mode_setup;
	em#:=10pt#; cap#:=13pt#;
define_pixels(em,cap);
buffer=round 0.1em;
pickup pencircle scaled 10; ver1_pen:=savepen;
beginchar("a",.75em#,cap#,0); "Lowercase a";
	pickup ver1_pen;
	x0=x1=buffer;
	x2=x7=x6=w/2;
	x3=x4=x5=w-buffer;
	y6=y5=0;
	y0=y4=.2h;
	y1=y7=y3=.4h;
	y2=.6h;
	draw z0--z7--z4--z6--z0;
	draw z1--z2--z3--z5;
	labels(range 0 thru 7);
endchar;

beginchar("b",.75em#,cap#,0); "Lowercase b";
	pickup ver1_pen;
	x0=x1=x2=buffer;
	x5=x3=w/2;
	x4=w-buffer;
	y0=y5=0;
	y1=y4=.3h;
	y3=.6h;
	y2=h;
	draw z0--z2;
	draw z1--z3--z4--z5--z1;
	labels(range 0 thru 5);
endchar;

beginchar("c",.5em#,cap#,0); "Lowercase c";
	pickup ver1_pen;
	x0=buffer;
	x1=x2=w-buffer;
	y0=.3h; y2=0; y1=.6h;
	draw z2--z0--z1;
	labels(range 0 thru 2);
endchar;

beginchar("d",.75em#,cap#,0); "Lowercase d";
	pickup ver1_pen;
	x1=buffer;
	x5=x3=w/2;
	x0=x2=x4=w-buffer;
	y0=y5=0;
	y1=y4=.3h;
	y3=.6h;
	y2=h;
	draw z0--z2;
	draw z1--z3--z4--z5--z1;
	labels(range 0 thru 5);
endchar;

beginchar("e",.75em#,cap#,0); "Lowercase e";
	pickup ver1_pen;
	x0=buffer;
	x1=x2=w/2;
	x3=w-buffer;
	y0=y3=.3h; y2=0; y1=.6h;
	draw z2--z0--z1--z3--z0;
	labels(range 0 thru 3);
endchar;

beginchar("f",.5em#,cap#,.4cap#); "Lowercase f";
	pickup ver1_pen;
	x0=x1=buffer;
	x2=x3=w-buffer;
	y0=-.4h;
	y1=y3=.3h;
	y2=.6h;
	draw z0--z1--z2;
	draw z1--z3;
	labels(range 0 thru 3);
endchar;

beginchar("g",.75em#,cap#,.4cap#); "Lowercase g";
	pickup ver1_pen;
	x0=x7=buffer;
	x1=x2=x6=w/2;
	x3=x4=x5=w-buffer;
	y0=y3=.3h; 
	y2=y5=y7=0; 
	y1=y4=.6h;
	y6=-.3h;
	draw z2--z0--z1--z3--z2;
	draw z4--z5--z6--z7;
	labels(range 0 thru 7);
endchar;

beginchar("h",.5em#,cap#,0); "Lowercase h";
	pickup ver1_pen;
	x0=x1=x2=buffer;
	x3=x4=w-buffer;
	y0=y4=0;
	y1=.3h;
	y2=h;
	y3=.6h;
	draw z0--z2;
	draw z1--z3--z4;
	labels(range 0 thru 4);
endchar;

beginchar("i",.25em#,cap#,0); "Lowercase i";
	pickup ver1_pen;
	x0=x1=x2=x3=w/2;
	y0=0;
	y1=.6h;
	y2=.8h;
	y3=.9h;
	draw z0--z1;
	draw z2--z3;
	labels(range 0 thru 3);
endchar;

beginchar("j",.5em#,cap#,.4cap#); "Lowercase j";
	pickup ver1_pen;
	x0=buffer;
	x1=x2=x3=x4=w-buffer;
	y0=-.1h;
	y1=-.4h;
	y2=.6h;
	y3=.8h; y4=.9h;
	draw z0--z1--z2;
	draw z3--z4;
	labels(range 0 thru 4);
endchar;

beginchar("k",.5em#,cap#,0); "Lowercase k";
	pickup ver1_pen;
	x0=x3=x4=buffer;
	x1=x2=w-buffer;
	y0=.3h; y2=y3=0; y1=.6h; y4=h;
