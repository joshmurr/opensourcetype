	x4=.5w;
	x2=x3=w;
	y4=0;
	y0=y3=.2h;
	y1=y2=h;
	draw z0..z1;
	draw z0..z4;
	draw z4..z3;
	draw z3..z2;
endchar;

beginchar("V",.75em#,cap#,0); "Uppercase V";
