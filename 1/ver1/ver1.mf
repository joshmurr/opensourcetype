beginchar("R",.75em#,cap#,0); "Uppercase R";
	pickup ver1_pen;
	x0=x1=x2=0;
	x3=x5=.5w;
	x4=x6=w;
	y0=y6=0;
	y1=y5=.6h;
	y4=.8h;
	y2=y3=h;
	draw z0..z2;
	draw z2..z3;
	draw z3..z4;
	draw z4..z5;
	draw z5..z1;
	draw z5..z6;
