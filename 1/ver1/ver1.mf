	draw z1..z4;
	draw z3..z5;
	labels(range 0 thru 5);
endchar;

beginchar("I",.25em#,cap#,0); "Uppercase I";
	pickup ver1_pen;
	z0=(.5w,0);
	z1=(.5w,h);
	draw z0..z1;
endchar;

beginchar("J",.75em#,cap#,0); "Uppercase J";
	pickup ver1_pen;
	x0=x1=0;
	x2=x3=w;
	x4=.5w;
	y4=0;
	y1=y2=h;
	y0=y3=.2h;
	draw z1..z2;
	draw z2..z3;
	draw z3..z4;
	draw z4..z0;
	labels(range 0 thru 4)
endchar;

beginchar("K",.75em#,cap#,0); "Uppercase K";
	pickup ver1_pen;
	x0=x1=x2=x3=0;
	x4=x6=w;
	y0=y6=0;
	y3=y4=h;
	y1=.6h;
	y2=.8h;
	z5=whatever[z2,z6];
	z5=whatever[z1,z4];
	draw z0..z3;
	draw z1..z4;
	draw z5..z6;
	labels(range 1 thru 6);
endchar;

beginchar("L",.75em#,cap#,0); "Uppercase L";
