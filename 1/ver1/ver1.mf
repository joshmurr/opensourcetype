	z2=(w,h);
	draw z1..z0;
	draw z0..z2;
endchar;

beginchar("W",em#,cap#,0); "Uppercase W";
	pickup ver1_pen;
	x0=0; x1=.25w; x2=.5w; x3=.75w; x4=w;
