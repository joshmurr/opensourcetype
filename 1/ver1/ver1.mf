beginchar("T",.75em#,cap#,0); "Uppercase T";
	pickup ver1_pen;
	z0=(.5w,0);
	z1=(0,h);
	z2=(w,h);
	z3=(.5w,h);
	draw z1..z2;
	draw z0..z3;
endchar;

beginchar("U",.75em#,cap#,0); "Uppercase U";
