	x2=x5=x6=.5w;
	x3=x4=x7=w;
	y5=0=y7;
	y0=y4=y6=.2h;
	y1=y3=.8h;
	y2=h;
	draw z0..z1;
	draw z1..z2;
	draw z2..z3;
	draw z3..z4;
	draw z4..z5;
	draw z5..z0;
	draw z6..z7;
