mode_setup;
	em#:=8pt#; cap#:=10pt#;
define_pixels(em,cap);
pickup pencircle scaled 10; ver1_pen:=savepen;
beginchar("A",em#,cap#,0); "Uppercase A";
	pickup ver1_pen;
	y1=y5=0;
	y3=h;
	x1=0; x5=w; x3=w/2;
	y2=y4;
	z2=whatever[z1,z3];
	z4=whatever[z3,z5];
	(y3-y2)=(3/5)*(y2-y1);
	draw z1..z3;
	draw z3..z5;
	draw z2..z4;
	labels(range 0 thru 5);
endchar;

beginchar("B",.75em#,cap#,0); "Uppercase B";
	pickup ver1_pen;
	x0=x1=0;
	x2=x8=x4=.5w;
	x3=x6=x7=w;
	x5=w/3;
	y0=y8=0;
	y1=y2=h;
	y3=.8h;
	y5=y4=0.6h;
	y6=.4h;
	y7=.2h;
	draw z0..z1;
	draw z1..z2;
	draw z2..z3;
	draw z3..z4;
	draw z4..z5;
	draw z4..z6;
	draw z6..z7;
	draw z7..z8;
	draw z8..z0;
	labels(range 0 thru 8);
endchar;

beginchar("C",.75em#,cap#,0); "Uppercase C";
	pickup ver1_pen;
	x0=x1=0;
	x2=x5=.5w;
	x3=x4=w;
	y5=0; y2=h;
	y0=y4=.2h;
	y1=y3=.8h;
	draw z0..z1;
	draw z1..z2;
	draw z2..z3;
	draw z4..z5;
	draw z0..z5;
	labels(range 0 thru 5)
endchar;

beginchar("D",.75em#,cap#,0); "Uppercase D";
	pickup ver1_pen;
	x0=x1=0;
	x2=x3=w;
	y0=0;
	y1=h;
	y2=.8h;
	y3=.2h;
	draw z0..z1;
	draw z1..z2;
	draw z2..z3;
	draw z3..z0;
	labels(range 0 thru 3);
endchar;

beginchar("E",.75em#,cap#,0); "Upperase E";
	pickup ver1_pen;
	x0=x1=x2=0;
	x3=x5=w;
	x4=2w/3;
	y0=y5=0;
	y1=y4=.6h;
	y2=y3=h;
	draw z0..z2;
	draw z2..z3;
	draw z1..z4;
	draw z0..z5;
	labels(range 0 thru 5);
endchar;

beginchar("F",.75em#,cap#,0); "Upperase F";
	pickup ver1_pen;
	x0=x1=x2=0;
	x3=x5=w;
	x4=2w/3;
	y0=0;
	y1=y4=.6h;
	y2=y3=h;
	draw z0..z2;
	draw z2..z3;
	draw z1..z4;
	labels(range 0 thru 5);
endchar;

beginchar("G",.75em#,cap#,0); "Uppercase G";
	pickup ver1_pen;
	x0=x1=0;
	x2=x5=.5w;
	x3=x4=x6=w;
	x7=w/3;
	y5=0; y2=h;
	y0=y4=.2h;
	y1=y3=.8h;
	y6=y7=.6h;
	draw z0..z1;
	draw z1..z2;
	draw z2..z3;
	draw z4..z5;
	draw z0..z5;
	draw z4..z6;
	draw z6..z7;
	labels(range 0 thru 7)
endchar;

beginchar("H",.75em#,cap#,0); "Upperase H";
	pickup ver1_pen;
