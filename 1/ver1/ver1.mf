	y0=y4=0; y1=y3=h; y2=.2h;
	draw z0..z1;
	draw z1..z2;
	draw z2..z3;
	draw z3..z4;
endchar;

beginchar("N",.75em#,cap#,0); "Uppercase N";
	pickup ver1_pen;
