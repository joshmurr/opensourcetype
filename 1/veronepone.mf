	x3=w-buffer;
	y0=0; y1=y3=.6h; y2=h;
	draw z0--z2;
	draw z1--z3;
	labels(range 0 thru 3);
endchar;

beginchar("u",.75em#,cap#,0); "Lowercase u";
	pickup ver1_pen;
	x0=x1=buffer;
	x2=w/2;
	x3=x4=w-buffer;
	y2=0;
	y1=y3=.3h;
	y0=y4=.6h;
	draw z0--z1--z2--z3--z4;
	labels(range 0 thru 4);
endchar;

beginchar("v",.75em#,cap#,0); "Lowercase v";
	pickup ver1_pen;
	x0=buffer;
	x1=w/2;
	x2=w-buffer;
	y1=0;
	y0=y2=.6h;
	draw z0--z1--z2;
	labels(range 0 thru 2);
endchar;

beginchar("w",.75em#,cap#,0); "Lowercase w";
	pickup ver1_pen;
	x0=x1=buffer;
	x2=w/2;
	x3=x4=w-buffer;
	y0=y4=0;
	y1=y3=.6h;
	y2=.3h;
	draw z1--z0--z2--z4--z3;
	labels(range 0 thru 4);
endchar;

beginchar("x",.75em#,cap#,0); "Lowercase x";
	pickup ver1_pen;
	x0=x1=buffer;
	x2=x3=w-buffer;
	y0=y3=0;
	y1=y2=.6h;
	draw z0--z2;
	draw z1--z3;
	labels(range 0 thru 3);
endchar;

beginchar("y",.5em#,cap#,.4cap#); "Lowercase y";
	pickup ver1_pen;
	x0=buffer;
	x1=x2=x3=w-buffer;
	y0=y1=.6h;
	y2=.3h;
